@article{PerssonStrang04,
	Abstract = {Abstract. Creating a mesh is the first step in a wide range of applications,
including scientific computing and computer graphics. An unstructured
simplex mesh requires a choice of meshpoints (vertex nodes) and a
triangulation. We want to offer a short and simple MATLAB code, described
in more detail than usual, so the reader can experiment (and add
to the code) knowing the underlying principles. We find the node
locations by solving for equilibrium in a truss structure (using
piecewise linear force-displacement relations) and we reset the topology
by the Delaunay algorithm. The geometry is described implicitly by
its distance function. In addition to being much shorter and simpler
than other meshing techniques, our algorithm typically produces meshes
of very high quality. We discuss ways to improve the robustness and
the performance, but our aim here is simplicity. Readers can download
(and edit) the codes from http://math.mit.edu/{\~{}}persson/mesh.},
	Annote = {This paper describes a simple scheme for generating a mesh to be
used, for example, in the numerical solution of PDEs. The basic idea
is to treat each mesh point as a point mass and to exert forces on
the point masses that spread the points apart while keeping them
sufficiently close, and also minimzing a cost that is a function
of the geometry.},
	Author = {Persson, Per O and Strang, Gilbert},
	File = {:Users/beard/Dropbox/library/PerssonStrang04.pdf:pdf},
	Journal = {{SIAM} Review},
	Keywords = {Delaunay triangulation,mesh generation},
	Number = {2},
	Pages = {329--345},
	Title = {{A Simple Mesh Generator in {MATLAB}}},
	Volume = {46},
	Year = {2004}}

