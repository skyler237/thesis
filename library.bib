	File = {:Users/beard/Dropbox/library/LawranceSukkarieh10.pdf:pdf},
	Keywords = {SLAM,Wind field,thermals,wind estimation},
	Month = {aug},
	Number = {AIAA-2010-8032},
	Title = {{Simultaneous Exploration and Exploitation of a Wind Field for a Small Gliding {UAV}}},
	Year = {2010}}

