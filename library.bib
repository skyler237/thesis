@inproceedings{Orr2005,
	Abstract = {An investigation has begun to evaluate the behavior of Small Unmanned
Aerial Vehicles (SUAVs) and Micro Aerial Vehicles (MAVs) flying through
an urban setting. This research is focused on a cooperative scenario
between a SUAV and MAVs for the Air Force Research Laboratory (AFRL)
