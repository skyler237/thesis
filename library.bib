@article{KongMettler12_submitted,
	Abstract = {Abstract---This paper presents a unified, formal framework
to help analyze and model human guidance behavior. In this
framework, the guidance behavior is analyzed as the closed-loop
dynamics of the whole agent-environment system. The resulting
coupled agent-environment dynamics give rise to interaction
patterns. The central hypothesis is that these patterns, which
can be explained in terms of invariances inherent to closed-loop
behavior, provide the building blocks for the organization of
behavior. The paper first describes these concepts using a toy
model and then details the technique that was developed for
the analysis of experimental data collected using a miniature
helicopter in agile human guidance experiments. The results
confirm that human guidance behavior indeed exhibits invariances
as defined by interaction patterns. The behavior in each
interaction pattern is then investigated using piecewise linear
model identification. Combined these two steps provide a natural
and formal decomposition of the behavior that can be unified
under a hybrid automaton.},
	Annote = {Formally modeling the way that humans control and manipulate robots is a challenging and difficult problem, and so the goals of this paper are quite ambitious and have the potential for high payoff. The basic idea of the paper is to model human controlled behavior of mobile robots using two equivalence relationships, namely similar trajectories (after a transformation), and sub-goal behavior. The idea of using similar trajectories is in keeping with Dr. Mettler{\&}{\#}039;s other work on motion primitives, and represents the common thread through most of her work. While this paper does not solve all of the issues that need to be addressed, it does represent an interesting and very reasonable approach to the problem. For me, the most impressive aspect of this paper was the breadth of statistical and engineering tools that brought to bear on the problem.},
	Author = {Kong, Zhaodan and Mettler, Berenice},
	File = {:Users/beard/Dropbox/library/KongMettler12{\_}submitted.pdf:pdf},
	Journal = {IEEE Transactions on Systems, Man, and Cybernetics},
	Keywords = {Human robot interaction,motion primative automaton},
	Title = {{Interaction Patterns in Agent-Environment Dynamics and Foundations for Formal Modeling of Human Guidance Behavior}}}

@article{GoncalvesPimentaMaia10,
	Abstract = {Abstract---This paper presents a methodology for computation

