@article{Nielsen17,
author = {Jerel Nielsen and Randal Beard},
title = {Relative target estimation using a cascade of extended Kalman filters},
year = {2017},
    abstract = { This paper presents a method of estimating inertial target positions from a moving vehicle, without any assumptions on the shape of the terrain or the target motion. The tracking vehicle runs two cascaded estimators. The first is an Extended Kalman Filter (EKF), which is responsible for tracking the vehicleâ€™s state. The second estimator is an EKF that is responsible for estimating a fixed number of landmarks within the cameraâ€™s field of view. Landmarks are parameterized by a quaternion associated with bearing from the cameraâ€™s optical axis and an inverse distance parameter. The bearing quaternion allows for a minimal representation of each landmarkâ€™s direction and distance, a filter with no singularities, and a faster update rate due to few trigonometric functions. Three methods for estimating the inertial target positions are demonstrated: the first uses the landmark estimator directly on a moving target, the second computes the target depth with a weighted average of converged landmark depths, and the third extends the targetâ€™s measured unit vector to intersect a ground plane constructed from the converged landmark estimates. }
}

@article{Mahony11,
author = {R. Mahony and M. Euston and Jonghyuk Kim and P. Coote and T. Hamel},
title = {A non-linear observer for attitude estimation of a fixed-wing unmanned aerial vehicle without GPS measurements},
journal = {Transactions of the Institute of Measurement and Control},
volume = {33},
number = {6},
pages = {699-717},
year = {2011},
doi = {10.1177/0142331209343660},
URL = {
        http://dx.doi.org/10.1177/0142331209343660

},
eprint = {
        http://dx.doi.org/10.1177/0142331209343660

},
    abstract = { In this paper we propose a simple non-linear observer for attitude estimation based only on output from a typical inertial measurement unit (IMU) and dynamic pressure sensor embarked on a low-cost unmanned aerial vehicle. In particular, we aim to provide a good quality attitude estimate in the absence of global positioning system (GPS) ground truth and with potential low-frequency bias and high-frequency noise in the IMU sensor measurements. In addition, the case where the IMU only provides gyrometer and accelerometer outputs is considered; that is, there is no magnetometer output or it cannot be used due to local magnetic disturbances such as are common on a vehicle with electric motors. The proposed observer uses a simple centripetal force model (based on gyrometer and dynamic pressure measurements), augmented by a first-order dynamic model for angle of attack, to estimate non-inertial components of the acceleration. This estimate is used to correct the accelerometer output to provide a low-frequency estimate of the gravitational direction. This inertial direction, along with the gyrometer output, is then used to drive a fully non-linear attitude observer posed on the orthogonal group of rotation matrices SO(3). The observer is augmented with an integral state that ensures compensation of gyrometer bias. The resulting observer is simple to implement and fully non-linear. Experimental results are provided on a real-world data set and the performance of the filter is evaluated against the output from a full GPS/inertial navigation system (INS) that was available for the data set. }
}

