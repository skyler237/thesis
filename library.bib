@article{Sugar2002,
	Abstract = {We describe a framework and control algorithms for coordinating
multiple mobile robots with manipulators focusing on tasks that require
grasping, manipulation and transporting large and possibly flexible
objects without special purpose fixtures. Because each robot has an
independent controller and is autonomous, the coordination and synergy
are realized through sensing and communication. The robots can
